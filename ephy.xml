<?xml version="1.0" encoding="utf-8"?>

<device schemaVersion="1.1" xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:noNamespaceSchemaLocation="CMSIS-SVD_Schema_1_1.xsd">
    <name>EPHY</name>
    <version>1</version>
    <description>Ethernet PHY Peripherals</description>
    <addressUnitBits>5</addressUnitBits>
    <width>16</width>
    <size>16</size>


    <peripherals>
        <peripheral>
            <name>EPHY</name>
            <description>Ethernet PHY</description>
            <groupName>EPHY</groupName>
            <baseAddress>0</baseAddress>
            <addressBlock>
                <offset>0</offset>
                <size>0xFFF</size>
                <usage>registers</usage>
            </addressBlock>
            <registers>
                <register>
                    <name>BMCR</name>
                    <description>Desc</description>
                    <addressOffset>0x000</addressOffset>
                    <access>read-write</access>
                    <resetValue>0x00</resetValue>
                    <resetMask>0xFFFFFFFF</resetMask>
                    <fields>
                        <field>
                            <name>MIIRESET</name>
                            <bitRange>[15:15]</bitRange>
                            <resetValue>0</resetValue>
                        </field>
                        <field>
                            <name>MIILOOPBK</name>
                            <bitRange>[14:14]</bitRange>
                            <resetValue>0</resetValue>
                        </field>
                        <field>
                            <name>SPEED</name>
                            <bitRange>[13:13]</bitRange>
                            <resetValue>1</resetValue>
                        </field>
                        <field>
                            <name>ANEN</name>
                            <bitRange>[12:12]</bitRange>
                            <resetValue>1</resetValue>
                        </field>
                        <field>
                            <name>PWRDWN</name>
                            <bitRange>[11:11]</bitRange>
                            <resetValue>0</resetValue>
                        </field>
                        <field>
                            <name>ISOLATE</name>
                            <bitRange>[10:10]</bitRange>
                            <resetValue>0</resetValue>
                        </field>
                        <field>
                            <name>RESTARTAN</name>
                            <bitRange>[9:9]</bitRange>
                            <resetValue>0</resetValue>
                        </field>
                        <field>
                            <name>DUPLEXM</name>
                            <bitRange>[8:8]</bitRange>
                            <resetValue>1</resetValue>
                        </field>
                        <field>
                            <name>COLLTST</name>
                            <bitRange>[7:7]</bitRange>
                            <resetValue>0</resetValue>
                        </field>
                        <!-- <field>
                            <name>RESERVED</name>
                            <description>Reserved. The value read from a reserved bit is not defined.</description>
                            <bitRange>[0:6]</bitRange>
                        </field> -->
                    </fields>
                </register>
                <register>
                    <name>BMSR</name>
                    <description>Desc</description>
                    <addressOffset>0x002</addressOffset>
                    <access>read-only</access>
                    <resetValue>0x00</resetValue>
                    <resetMask>0xFFFFFFFF</resetMask>
                    <fields>
                        <field>
                            <name>100BTXFD</name>
                            <bitRange>[14:14]</bitRange>
                        </field>
                        <field>
                            <name>100BTXHD</name>
                            <bitRange>[13:13]</bitRange>
                        </field>
                        <field>
                            <name>10BTFD</name>
                            <bitRange>[12:12]</bitRange>
                        </field>
                        <field>
                            <name>10BTHD</name>
                            <bitRange>[11:11]</bitRange>
                        </field>
                        <field>
                            <name>MFPRESUP</name>
                            <bitRange>[6:6]</bitRange>
                        </field>
                        <field>
                            <name>ANC</name>
                            <bitRange>[5:5]</bitRange>
                        </field>
                        <field>
                            <name>RFAULT</name>
                            <bitRange>[4:4]</bitRange>
                        </field>
                        <field>
                            <name>ANEN</name>
                            <bitRange>[3:3]</bitRange>
                        </field>
                        <field>
                            <name>LINKSTAT</name>
                            <bitRange>[2:2]</bitRange>
                        </field>
                        <field>
